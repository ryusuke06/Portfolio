<div class="nested-fields well well-compact">
  <table>
  <div class="form-group">
    <tr>
      <th><%= f.label :title, "結果" %></th>
      <td><%= f.text_field :title, :size=>"60" %></td>
    </tr>
  </div>
  <div class="form-group">
    <tr>
      <th><%= f.label :caption, "結果説明文" %></th>
      <td><%= f.text_area :caption, :size=>"63" %></td>
    </tr>
  </div>
  <div class="form-group">
    <tr>
      <th><%= f.label :youtube_url, "youtubeURL" %></th>
      <td><%= f.url_field :youtube_url, :size=>"60" %></td>
    </tr>
    <tr>
      <th><%= f.label :pattern, "解答順" %></th>
      <td><% f.text_field :pattern, :size=>60  %></td>
      <td><%= link_to_remove_association "削除", f %></td>
    </tr>
  </div>
  </table>
</div>